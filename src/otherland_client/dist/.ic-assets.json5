[
  {
    "match": "*.html",
    "headers": {
      "Content-Security-Policy": "default-src 'self'; script-src 'self' https://cdn.jsdelivr.net; object-src 'none'; connect-src 'self' 0.peerjs.com wss://0.peerjs.com http://localhost:4943 https://icp0.io blob:; font-src 'self' data:",
      "Permissions-Policy": "xr-spatial-tracking=(self)"
    },
    "security_policy": "hardened"
  },
  {
    "match": "*.js",
    "headers": {
      "Content-Security-Policy": "default-src 'self'; script-src 'self'; object-src 'none'; connect-src 'self' https://icp0.io;"
