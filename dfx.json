{
  "canisters": {
    "OTHERLAND_NODE_backend": {
      "main": "src/OTHERLAND_NODE_backend/manager.mo",
      "type": "motoko"
    },
    "Storage": {
      "main": "src/OTHERLAND_NODE_backend/storage.mo",
      "type": "motoko"
    },
    "OTHERLAND_NODE_frontend": {
      "dependencies": ["OTHERLAND_NODE_backend", "Storage"],
      "source": ["src/OTHERLAND_NODE_frontend/dist"],
      "type": "assets",
      "workspace": "OTHERLAND_NODE_frontend",
      "headers": {
        "Content-Security-Policy": "default-src 'self'; script-src 'self' https://cdn.jsdelivr.net https://unpkg.com; object-src 'none';"
      }
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    }
  },
  "version": 1
}